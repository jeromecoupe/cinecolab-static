{% set metaTitle = metaTitle|default("") %}
{% set metaImage = metaImage|default("/assets/img/socialmedia.jpg") %}
{% set metaDescription = metaDescription|default("") %}

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>{{ metaTitle }} - Cin√©colab</title>
  <meta name="description" content="{{ metaDescription }}">
  <link rel="apple-touch-icon" href="/apple-touch-icon.png">
  <!-- Open Graph -->
  <meta property="og:title" content="{{ metaTitle }}">
  <meta property="og:type" content="website">
  <meta property="og:url" content="{{ page.url }}">
  <meta property="og:image" content="{{ metaImage }}">
  <!-- CSS -->
  <link rel="stylesheet" href="/assets/css/main.css">
  <!-- JS -->
  <script type="module" src="/assets/js/main.bundle.js"></script>
</head>